    "install": {
      "npmSpec": "wemp",
      "localPath": "extensions/wemp",
      "defaultChoice": "local"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "openclaw": "^2026.1.30"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
